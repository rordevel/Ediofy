.panel
  %h3 Rating Detail
  .panel_contents
    #attributes_table_friendship_badge_9.attributes_table.friendship_badge
      %table{border: "0", cellpadding: "0", cellspacing: "0"}
        %tbody
          %tr.row.row-name
            %th Content Title
            %td= params[:content_title]
          %tr.row.row-icon
            %th Type
            %td= params[:type]
          %tr.row.row-points
            %th Thumb Down
            %td= params[:thumb_down]
          %tr.row.row-type
            %th Last Voted Thumbs Down
            %td= DateTime.parse(params[:last_voted_thumbs_down]).strftime("%B %d, %Y %H:%M:%S")
      .panel
        %h3 Users
        .panel_contents
          %table{border: "0", cellpadding: "0", cellspacing: "0"}
            %thead
              %tr
                %th Name
                %th Email
                %th Voted Date
            %tbody
              - @resource.get_dislikes.each do |vote|
                %tr
                  %td= link_to vote.voter.full_name, admin_user_path(vote.voter.id)
                  %td= vote.voter.email
                  %td= vote.created_at.strftime("%B %d, %Y %H:%M:%S")